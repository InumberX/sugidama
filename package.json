{
  "name": "@InumberX/sugidama",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "npm run react-router-typegen && tsc -b",
    "format": "prettier --check \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "format-fix": "prettier --write \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "lint": "eslint \"./{app,tests,stories}/**/*.{ts,tsx}\" --max-warnings=0",
    "lint-fix": "eslint --fix \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "lint-quiet": "eslint --quiet \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "test": "vitest",
    "storybook": "storybook dev --no-open -p 6006 -c .storybook",
    "upgrade": "npx npm-check-updates -u",
    "upgrade-check": "npx npm-check-updates",
    "pre-commit": "npm run typecheck && npm run lint-fix && npm run format-fix",
    "react-router-typegen": "react-router typegen",
    "react-router-typegen-watch": "react-router typegen --watch"
  },
  "dependencies": {
    "@react-router/express": "^7.11.0",
    "@react-router/fs-routes": "^7.11.0",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "cross-env": "^10.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "framer-motion": "^12.23.26",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fs-backend": "^2.6.1",
    "i18next-http-backend": "^3.0.2",
    "isbot": "^5.1.32",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0",
    "react-router": "^7.11.0",
    "remix-i18next": "^7.4.2"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@react-router/dev": "^7.11.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vitejs/plugin-react": "^5.1.2",
    "csstype": "^3.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-junit": "^9.0.1",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.7.4",
    "storybook": "^10.1.10",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "volta": {
    "node": "24.5.0"
  }
}
