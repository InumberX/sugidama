{
  "name": "sugidama",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "postbuild": "chmod +x bin/postbuild.sh && ./bin/postbuild.sh",
    "typecheck": "tsc -b",
    "format": "prettier --check \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "format-fix": "prettier --write \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "lint": "eslint \"./{app,tests,stories}/**/*.{ts,tsx}\" --max-warnings=0",
    "lint-fix": "eslint --fix \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "lint-quiet": "eslint --quiet \"./{app,tests,stories}/**/*.{ts,tsx}\"",
    "test": "vitest",
    "storybook": "storybook dev --no-open -p 6006 -c .storybook"
  },
  "dependencies": {
    "@remix-run/express": "^2.13.1",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "framer-motion": "^11.11.9",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@remix-run/dev": "^2.13.1",
    "@remix-run/testing": "^2.13.1",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vanilla-extract/css": "^1.16.0",
    "@vanilla-extract/vite-plugin": "^4.0.16",
    "@vitejs/plugin-react": "^4.3.3",
    "csstype": "^3.1.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "storybook": "^8.3.6",
    "typescript": "5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "18.18.2"
  }
}
